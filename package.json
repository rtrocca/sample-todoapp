{
  "name": "ajstodo",
  "version": "0.0.1",
  "private": true,
  "description": "A simple Angular JS ToDo app to be used as sample",
  "author": "Riccardo Trocca",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "build-dev": "webpack --mode development",
    "watch-dev": "webpack -w --mode development",
    "watch": "npm-run-all --parallel watch-dev watch-types",
    "watch-types": "tsc -w",
    "check-types": "tsc",
    "lint": "eslint ./src/**/*.{js,jsx,ts,tsx}",
    "lint-fix": "eslint ./src/**/*.{js,jsx,ts,tsx} --fix"
  },
  "babel": {
    "presets": [
      "@babel/typescript",
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/proposal-class-properties",
      "@babel/proposal-object-rest-spread"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@uirouter/angularjs": "1.0.19",
    "@uirouter/core": "^6.0.4",
    "angular": "^1.7.5",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "popper.js": "^1.14.5",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-dom": "^16.12.0",
    "react2angular": "^4.0.6",
    "uuid4": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/angular": "^1.6.51",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.9.0",
    "node-notifier": "^5.3.0",
    "node-sass": "^4.13.1",
    "node-sass-import": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "run-sequence": "^2.2.1",
    "sass-loader": "^8.0.2",
    "serve": "^11.3.0",
    "source-map": "^0.7.3",
    "stringify": "^5.2.0",
    "style-loader": "^1.1.3",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
